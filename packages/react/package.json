{
  "name": "range-kit-react",
  "version": "1.0.4",
  "description": "React bindings for Range Kit",
  "homepage": "https://github.com/acsamson/range-kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/acsamson/range-kit.git",
    "directory": "packages/react"
  },
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/selection-popover.css"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.0",
    "range-kit": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^1.6.1",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tsup": "^8.0.1",
    "typescript": "^5.7.2",
    "vitest": "^1.6.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "README_zh.md",
    "LICENSE"
  ]
}
