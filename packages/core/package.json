{
  "name": "range-kit",
  "version": "1.0.4",
  "description": "Range Selection and Restoration Kit",
  "homepage": "https://github.com/acsamson/range-kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/acsamson/range-kit.git",
    "directory": "packages/core"
  },
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^1.6.1",
    "jsdom": "^27.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.7.2",
    "vitest": "^1.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "README_zh.md",
    "LICENSE"
  ]
}
