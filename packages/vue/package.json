{
  "name": "range-kit-vue",
  "version": "1.0.4",
  "description": "Vue bindings for Range Kit",
  "homepage": "https://github.com/acsamson/range-kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/acsamson/range-kit.git",
    "directory": "packages/vue"
  },
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist/selection-popover.css": "./dist/selection-popover.css",
    "./styles": "./dist/selection-popover.css"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@floating-ui/vue": "^1.1.9",
    "range-kit": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.7.2",
    "vue": "^3.3.8",
    "vitest": "^1.6.1",
    "@vitest/coverage-v8": "^1.6.1",
    "jsdom": "^27.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "README_zh.md",
    "LICENSE"
  ]
}
